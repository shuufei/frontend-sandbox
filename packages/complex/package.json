{
  "name": "@sandbox-frontend/complex",
  "version": "0.0.0",
  "scripts": {
    "test": "node ./dist/index.js",
    "build": "tsc index.ts --outDir ./"
  },
  "dependencies": {
    "@sandbox-frontend/simple": "*"
  },
  "devDependencies": {
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "nx": {
    "namedInputs": {
      "default": [
        "{projectRoot}/**/*"
      ]
    },
    "targets": {
      "build": {
        "inputs": ["default"]
      },
      "test": {
        "inputs": ["default"],
        "dependsOn": ["build", "^build"]
      }
    }
  }
}
